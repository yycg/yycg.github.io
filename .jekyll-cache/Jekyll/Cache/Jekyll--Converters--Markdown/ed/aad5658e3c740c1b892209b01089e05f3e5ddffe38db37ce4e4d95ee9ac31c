            <span class="n">cnt</span><span class="o">++</span><span class="p">;</span>
            <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">neighbour</span><span class="o">:</span> <span class="n">graph</span><span class="p">[</span><span class="n">node</span><span class="p">])</span> <span class="p">{</span>
                <span class="n">indegree</span><span class="p">[</span><span class="n">neighbour</span><span class="p">]</span><span class="o">--</span><span class="p">;</span>
