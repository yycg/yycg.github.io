        <span class="k">while</span><span class="p">(</span><span class="n">ss</span> <span class="o">&gt;&gt;</span> <span class="n">oprt</span> <span class="o">&gt;&gt;</span> <span class="n">opnd</span><span class="p">)</span> <span class="p">{</span>
            <span class="k">if</span><span class="p">(</span><span class="n">oprt</span> <span class="o">==</span> <span class="sc">'+'</span> <span class="o">||</span> <span class="n">oprt</span> <span class="o">==</span> <span class="sc">'-'</span><span class="p">)</span> <span class="p">{</span>
                <span class="n">res</span> <span class="o">+=</span> <span class="n">last</span><span class="p">;</span>
