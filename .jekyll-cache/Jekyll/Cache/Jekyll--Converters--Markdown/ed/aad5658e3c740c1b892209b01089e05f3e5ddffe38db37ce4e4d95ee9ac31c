            <span class="k">if</span><span class="p">(</span><span class="n">noConflicts</span><span class="p">(</span><span class="n">board</span><span class="p">,</span> <span class="n">row</span><span class="p">,</span> <span class="n">column</span><span class="p">,</span> <span class="n">num</span><span class="p">))</span> <span class="p">{</span>
