        <span class="n">vector</span><span class="o">&lt;</span><span class="kt">int</span><span class="o">&gt;</span> <span class="n">now</span><span class="p">(</span><span class="n">numRow</span><span class="p">,</span> <span class="mi">0</span><span class="p">);</span>
