        <span class="p">}</span>
        <span class="n">sort</span><span class="p">(</span><span class="n">nums</span><span class="p">.</span><span class="n">begin</span><span class="p">(),</span> <span class="n">nums</span><span class="p">.</span><span class="n">end</span><span class="p">());</span>
        <span class="n">backtrack</span><span class="p">(</span><span class="mi">0</span><span class="p">,</span> <span class="n">nums</span><span class="p">.</span><span class="n">size</span><span class="p">(),</span> <span class="n">nums</span><span class="p">,</span> <span class="n">combination</span><span class="p">,</span> <span class="n">available</span><span class="p">,</span> <span class="n">res</span><span class="p">);</span>
          
