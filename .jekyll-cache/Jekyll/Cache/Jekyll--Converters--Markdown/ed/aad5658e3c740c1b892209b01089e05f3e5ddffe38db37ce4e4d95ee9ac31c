        <span class="k">if</span><span class="p">(</span><span class="o">!</span><span class="n">findEmptyCell</span><span class="p">(</span><span class="n">board</span><span class="p">,</span> <span class="n">row</span><span class="p">,</span> <span class="n">column</span><span class="p">))</span> <span class="k">return</span> <span class="nb">true</span><span class="p">;</span>
