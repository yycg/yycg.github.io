            <span class="p">}</span>
        <span class="p">}</span>
        <span class="n">res</span> <span class="o">+=</span> <span class="n">last</span><span class="p">;</span>
        <span class="k">return</span> <span class="n">res</span><span class="p">;</span>
    <span class="p">}</span>
