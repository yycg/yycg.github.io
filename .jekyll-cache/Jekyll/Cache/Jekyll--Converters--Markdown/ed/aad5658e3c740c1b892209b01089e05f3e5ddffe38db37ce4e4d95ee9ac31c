        <span class="p">}</span>
        <span class="k">return</span> <span class="n">res</span><span class="p">.</span><span class="n">empty</span><span class="p">()</span><span class="o">?</span> <span class="s">"0"</span><span class="o">:</span> <span class="n">res</span><span class="p">;</span>
    <span class="p">}</span>
<span class="p">};</span>
