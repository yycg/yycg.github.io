                <span class="n">last</span> <span class="o">+=</span> <span class="n">oprt</span><span class="o">==</span><span class="sc">'+'</span><span class="o">?</span> <span class="n">opnd</span><span class="o">:</span> <span class="o">-</span><span class="n">opnd</span><span class="p">;</span>
