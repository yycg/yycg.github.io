            <span class="k">else</span> <span class="n">hi</span> <span class="o">=</span> <span class="n">mid1</span><span class="p">;</span>
        <span class="p">}</span>
        <span class="k">return</span> <span class="n">lo</span><span class="p">;</span>
    <span class="p">}</span>
